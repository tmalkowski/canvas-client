{
  "name": "canvas-client",
  "version": "1.2.0",
  "description": "Typescript library to make accessing the Canvas API more convenient.",
  "main": "lib/index.js",
  "module": "lib-esm/index.js",
  "browser": {
    "./lib/index.js": "./lib/browser.js",
    "./lib-esm/index.js": "./lib-esm/browser.js"
  },
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "rm -rf lib lib-esm",
    "build": "tsc && tsc -m es6 --outDir lib-esm --moduleResolution node",
    "test": "mocha -r ts-node/register test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txstate-etc/canvas-client.git"
  },
  "keywords": [],
  "author": "Nick Wing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/txstate-etc/canvas-client/issues"
  },
  "homepage": "https://github.com/txstate-etc/canvas-client#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.157",
    "@types/luxon": "^1.24.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.14",
    "@types/parse-link-header": "^1.0.0",
    "@types/qs": "^6.9.3",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.1",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "luxon": "^1.24.1",
    "mocha": "^8.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "agentkeepalive": "^4.1.3",
    "axios": "^0.19.2",
    "lodash": "^4.17.15",
    "p-limit": "^3.0.1",
    "parse-link-header": "^1.0.1",
    "qs": "^6.9.4"
  },
  "files": [
    "lib/**/*",
    "lib-esm/**/*"
  ]
}
